<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="pictures" namespace="/" extends="json-default">
		<action name="PicturesAction_*" class="picturesAction" method="{1}">
			<result name="query" type="json">
				<param name="ignoreHierarchy">false</param>
				<param name="includeProperties">pagination\.from,pagination\.size,pagination\.total,pagination\.nowpage
					,pagination\.rows\[\d+\]\.picturesId,pagination\.rows\[\d+\]\.messages\.messagesId,pagination\.rows\[\d+\]\.users\.usersId,
					pagination\.rows\[\d+\]\.picturesUrl,pagination\.rows\[\d+\]\.picturesType,pagination\.rows\[\d+\]\.picturesTime
				</param>
			</result>
			<result name="cutsucceed" type="json">
				<param name="root">flag</param>
			</result>
			<result name="deletePicturesSucceed" type="json">
				<param name="root">flag</param>
			</result>
			<result name="uploadsucceed" type="json">
				<param name="root">finalPath</param>
			</result>
			<result name="uploadfail" type="json">
				<param name="root">finalPath</param>
			</result>
			<result name="iosUploadHeadPic" type="json">
				<param name="root">result</param>
			</result>
			
			<result name="iosUploadFail" type="json">
				<param name="root">result</param>
			</result>
			<result name="queryall" type="json">
				<param name="root">list</param>
			</result>
			<result name="delete" type="json">
				<param name="root">flag</param>
			</result>
			<result name="saveorupdate" type="json">
				<param name="root">flag</param>
			</result>
		</action>
	</package>
</struts>