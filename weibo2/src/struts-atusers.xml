<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="atusers" namespace="/" extends="json-default">
		<action name="AtusersAction_*" class="atusersAction" method="{1}">
			<result name="query" type="json">
				<param name="ignoreHierarchy">false</param>
				<param name="includeProperties">pagination\.from,pagination\.size,pagination\.total,pagination\.rows\[\d+\]\.atusersId,pagination\.rows\[\d+\]\.atusersTime,pagination\.rows\[\d+\]\.users\.usersId,pagination\.rows\[\d+\]\.messages\.messagesId</param>
			</result>
			<result name="queryall" type="json">
				<param name="root">list</param>
			</result>
			<result name="delete" type="json">
				<param name="root">flag</param>
			</result>
			<result name="saveorupdate" type="json">
				<param name="root">flag</param>
			</result>
		</action>
	</package>
</struts>